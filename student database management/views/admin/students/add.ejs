<!DOCTYPE html>
<html>
<head>
    <title>Add Students</title>
    <link rel="stylesheet" type="text/css" href="../css/styles.css">
</head>
<body>
<div class="path">
    <a href="/admin">admin panel</a> /
    <a>add students</a>
</div>
<h1>Add Students</h1>
<% if (warningMessage) { %>
    <p class='warning-message'>
        <% var warnings = warningMessage.split("\n");
        for (var i = 0; i < warnings.length; i++) { if (!warnings[i].length) continue %>
        <%= warnings[i] %> <br>
        <% } %>
    </p>
<% } %>
<% if (successfulMessage) { %>
    <p class='successful-message'>
        <% var successfuls = successfulMessage.split("\n");
        for (var i = 0; i < successfuls.length; i++) { if (!successfuls[i].length) continue %>
        <%= successfuls[i] %> <br>
        <% } %>
    </p>
<% } %>
<form action="/admin/add-students" method="post">
    <label for="startID">From:</label>
    <input type="text" id="startID" name="startID" placeholder="Enter the beginning of the students' IDs">
    <br><br>
    <label for="endID">To:</label>
    <input type="text" id="endID" name="endID" placeholder="Enter the end of the students' IDs">
    <br><br>
    <input type="submit" value="Add">
</form>
</body>
</html>
